{
  "name": "lesscode",
  "version": "0.3.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib/client/dist",
    "clean-dll": "rimraf lib/client/static/lib*",
    "build": "better-npm-run build",
    "local": "better-npm-run local",
    "online": "better-npm-run online",
    "dll": "better-npm-run dll",
    "dev": "better-npm-run dev",
    "stop": "cross-env FOREVER_ROOT=./ forever stop lesscode",
    "list": "cross-env FOREVER_ROOT=./ forever list",
    "logs": "cross-env FOREVER_ROOT=./ forever logs"
  },
  "betterScripts": {
    "dll": {
      "command": "npm run clean-dll && node lib/client/build/build-dll.js"
    },
    "build": {
      "command": "npm run clean && npm run dll && node lib/client/build/build.js",
      "env": {
        "NODE_ENV": "production",
        "APP_CODE": "",
        "BKPAAS_ENVIRONMENT": "prod"
      }
    },
    "dev": {
      "command": "npm run dll && nodemon --config nodemon.json",
      "env": {
        "NODE_ENV": "development",
        "APP_CODE": ""
      }
    },
    "local": {
      "command": "npm run build && node lib/server/app.browser.js",
      "env": {
        "NODE_ENV": "local",
        "APP_CODE": ""
      }
    },
    "online": {
      "command": "npm run build && cross-env FOREVER_ROOT=./ forever --minUptime 1000 --spinSleepTime 2000 start forever.json",
      "env": {
        "NODE_ENV": "production",
        "APP_CODE": "",
        "BKPAAS_ENVIRONMENT": "prod"
      }
    }
  },
  "keywords": [
    "bk-magic",
