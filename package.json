{
  "name": "lesscode",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dbmigrate": "node node_modules/db-migrate/bin/db-migrate --config ./lib/server/conf/db-migrate.json --migrations-dir ./lib/server/model/migrations create update-sql --sql-file",
    "build": "better-npm-run build",
    "local": "better-npm-run local",
    "online": "better-npm-run online",
    "dev": "better-npm-run dev",
    "cleanCache": "rm -rf ./node_modules/.cache",
    "stop": "cross-env FOREVER_ROOT=./ forever stop lesscode",
    "list": "cross-env FOREVER_ROOT=./ forever list",
    "logs": "cross-env FOREVER_ROOT=./ forever logs"
  },
  "betterScripts": {
    "build": {
      "command": "node node_modules/db-migrate/bin/db-migrate up --config ./lib/server/conf/db-migrate.json --migrations-dir ./lib/server/model/migrations -e prod && bk-cli-service-webpack build",
      "env": {
        "NODE_ENV": "production",
        "APP_CODE": "",
        "BKPAAS_ENVIRONMENT": "prod"
      }
    },
    "dev": {
      "command": "node node_modules/db-migrate/bin/db-migrate up --config ./lib/server/conf/db-migrate.json --migrations-dir ./lib/server/model/migrations -e dev && node ./scripts/dev.js",
      "env": {
        "NODE_ENV": "development",
        "APP_CODE": ""
      }
    },
    "local": {
      "command": "npm run build && node lib/server/app.browser.js",
      "env": {
        "NODE_ENV": "local",
        "APP_CODE": ""
      }
    },
    "online": {
      "command": "npm run build && cross-env FOREVER_ROOT=./ forever --minUptime 1000 --spinSleepTime 2000 start forever.json",
      "env": {
        "NODE_ENV": "production",
        "APP_CODE": "",
        "BKPAAS_ENVIRONMENT": "prod"
      }
    }
  },
